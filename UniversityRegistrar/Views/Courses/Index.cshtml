@{
    Layout = "_Layout";
}


<h1 class="sub-page-headers">Courses</h1>
<h4><a href='/courses/new'>Create a new course</a></h4>

@using UniversityRegistrar.Models

@foreach (Course course in @Model)
{
    <p>@Html.ActionLink(course.CourseName, "Details", "Courses", new {id = course.CourseId})</p>
}